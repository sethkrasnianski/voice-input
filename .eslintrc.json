{
  "rules": {
    "no-param-reassign": [2, {
      "props": false
    }],
    "no-alert": 0,
    "comma-dangle": 0
  },
  "extends": "airbnb-base",
  "installedESLint": true,
  "plugins": [
    "import"
  ],
  "globals": {
    "document": true,
    "window": true,
    "navigator": true,
    "alert": true,
    "Recorder": true,
    "FormData": true,
    "$": true
  }
}
